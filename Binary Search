#include <stdio.h>
int binary_search(int nums[],int size,int target) {
    
	int left=0,right=size-1;

	while(left<=right)
	{
	    int mid;
		mid=(left+right)/2;
		if(nums[mid]==target)
		{
			return mid;
		}
		if(target>nums[mid])
		{
			left=mid+1;
			
		}
		else if(target<nums[mid])
		{
			right=mid-1;
		}
		else
		{
		    if(target>nums[size]&&target<nums[size])
		    {
		        size++;
		    }
		}
	}

}
int main()
{
    int size;
    printf("Enter the size of Array: ");
    scanf("%d",&size);
    int nums[size];
    printf("Enter the Array Elements: ");
    for(int i=0;i<size;i++)
    {
        scanf("%d",&nums[i]);
    }
    int target;
    printf("Enter the Target to be find: ");
    scanf("%d",&target);
    int result=binary_search(nums,size,target);
    if(result!=-1)
    {
        printf("Target %d found at index %d\n", target, result);
    }
    else
    {
        printf("Target %d not found\n", result);
    }
}

OUTPUT:

Enter the size of Array: 8
Enter the Array Elements: 2 3 4 5 6 8 9 11
Enter the Target to be find: 4

Target 4 found at index 2
